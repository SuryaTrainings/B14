        "description": "{{compDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui":{
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://home",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet":"",
            "tablet@2": ""
        },
